{
  "name": "nextjs-dev-env",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "dependencies": {
    "@formatjs/intl-pluralrules": "^2.2.6",
    "@formatjs/intl-relativetimeformat": "^5.2.6",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "intl": "^1.2.5",
    "isomorphic-unfetch": "^3.0.0",
    "lodash.get": "^4.4.2",
    "lodash.mapvalues": "^4.6.0",
    "next": "^9.4.5-canary.12",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^4.6.9",
    "react-redux": "^7.2.0",
    "redaction": "^5.1.1",
    "redux": "^4.0.5",
    "redux-batched-subscribe": "^0.1.6",
    "redux-form": "^8.3.6",
    "suncalc": "^1.8.0"
  },
  "devDependencies": {
    "docz": "^2.3.1",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^1.2.0",
    "eslint": "^7.2.0",
    "gh-pages": "^3.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "prettier": "^2.0.5",
    "react-scripts": "^3.4.1",
    "react-test-renderer": "^16.13.1",
    "serve": "^11.3.2",
    "stylelint": "^13.6.0",
    "stylelint-config-sass-guidelines": "^7.0.0"
  },
  "scripts": {
    "dev": "next ./src",
    "build:server": "rm -rf dist && next build ./src",
    "serve:static": "serve ./dist",
    "start": "next start ./src",
    "build:static": "npm run build:server && next export ./src -o dist",
    "lint:eslint": "eslint ./src",
    "lint:prettier": "prettier -c .",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "test": "SKIP_PREFLIGHT_CHECK=true react-scripts test",
    "docs": "docz dev",
    "predeploy": "npm run build:static",
    "deploy": "gh-pages -d build"
  }
}
