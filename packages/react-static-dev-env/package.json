{
  "name": "react-static-dev-env",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist artifacts tmp",
    "start": "npm run clean && react-static start",
    "stage": "cross-env NODE_ENV=development react-static build --staging",
    "build": "npm run clean && react-static build",
    "analyze": "react-static build --analyze",
    "serve": "serve dist -p 3000",
    "test": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts test",
    "lint": "eslint ./src",
    "docs": "docz dev",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@reach/router": "^1.3.3",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "intl": "^1.2.5",
    "isomorphic-unfetch": "^3.0.0",
    "lodash.get": "^4.4.2",
    "lodash.mapvalues": "^4.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^3.12.1",
    "react-redux": "^7.2.0",
    "redaction": "^5.1.1",
    "redux": "^4.0.5",
    "redux-batched-subscribe": "^0.1.6",
    "redux-form": "^8.3.6"
  },
  "devDependencies": {
    "babel-eslint": "9.x",
    "cross-env": "^6.0.3",
    "docz": "^1.3.2",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^1.0.0-rc.9",
    "eslint": "^6.8.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-config-react-tools": "1.x.x",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "gh-pages": "^2.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.21",
    "react-scripts": "^3.4.1",
    "react-static": "^7.4.0",
    "react-static-plugin-css-modules": "^7.2.2",
    "react-static-plugin-reach-router": "^7.4.0",
    "react-static-plugin-sass": "^7.3.0",
    "react-static-plugin-sitemap": "^7.4.0",
    "react-static-plugin-source-filesystem": "^7.4.0",
    "react-test-renderer": "^16.13.1",
    "serve": "11.2.0",
    "stylelint": "^11.1.1",
    "stylelint-config-sass-guidelines": "^6.2.0"
  }
}
