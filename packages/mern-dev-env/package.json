{
  "name": "@dalisoft/mern-dev-env",
  "version": "1.0.0",
  "description": "An modern and easy development environment using MERN stack",
  "module": "src/server.js",
  "type": "module",
  "engine": {
    "node": ">=12.16"
  },
  "private": true,
  "scripts": {
    "clean": "rm -rf build",
    "start": "node build/server.js",
    "start:dev": "NODE_ENV=development nodemon build/server.js",
    "test": "ava",
    "lint": "eslint ./src",
    "dev": "npm run clean && NODE_ENV=development rollup -c -w",
    "build": "npm run clean && rollup -c"
  },
  "keywords": [
    "nanoexpress",
    "modern",
    "dev-env"
  ],
  "author": "dalisoft",
  "license": "MIT",
  "dependencies": {
    "@formatjs/intl-pluralrules": "^2.2.9",
    "@formatjs/intl-relativetimeformat": "^5.2.9",
    "@nanoexpress/middlewares": "github:nanoexpress/middlewares",
    "ajv": "^6.12.2",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "consolemd": "^0.2.1",
    "cookie": "^0.4.1",
    "express-cors": "0.0.3",
    "express-jwt": "^5.3.3",
    "express-rate-limit": "^5.1.3",
    "fast-json-stringify": "^2.2.0",
    "helmet": "^3.23.2",
    "history": "^4.10.1",
    "intl": "^1.2.5",
    "isomorphic-unfetch": "^3.0.0",
    "lodash.get": "^4.4.2",
    "lodash.mapvalues": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "nanoexpress-pro": "^1.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-intl": "^4.7.2",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redaction": "^5.1.1",
    "redux": "^4.0.5",
    "redux-batched-subscribe": "^0.1.6",
    "redux-form": "^8.3.6",
    "suncalc": "^1.8.0",
    "swagger-ui-dist": "^3.27.0"
