    "@rollup/plugin-run": "^2.0.2",
    "autoprefixer": "^9.8.4",
    "ava": "^3.9.0",
    "babel-eslint": "^10.1.0",
    "dotenv-safe": "^8.2.0",
    "eslint": "^7.3.1",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "rollup": "^2.18.0",
    "rollup-plugin-postcss": "^3.1.2",
    "rollup-plugin-terser": "^6.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
