{
  "version": 2,
  "env": {
    "secretKey": "some-secret-password-at-least-32-characters-long",
    "jwtSecretKey": "some-secret-password-at-least-32-characters-long-2",
    "GRAPHIQL": "true"
  },
  "builds": [
    {
      "src": "src/vercel-lambda.js",
      "use": "@vercel/node",
      "config": {
        "helpers": false
      }
    },
    {
      "src": "public/*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/vercel-lambda.js"
    }
  ]
}
