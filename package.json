{
  "name": "docker-db-templates",
  "version": "0.1.0",
  "description": "Docker templates for most commonly used DB",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dalisoft/docker-db-templates.git"
  },
  "keywords": [
    "docker",
    "templates",
    "mongo",
    "postgres",
    "mysql",
    "redis",
    "dev-env"
  ],
  "author": "dalisoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dalisoft/docker-db-templates/issues"
  },
  "homepage": "https://github.com/dalisoft/docker-db-templates#readme",
  "devDependencies": {
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "prettier": "^1.17.0"
  }
}
