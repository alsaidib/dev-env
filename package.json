  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dalisoft/graphql-dev-env/issues"
  },
  "homepage": "https://github.com/dalisoft/graphql-dev-env#readme",
  "dependencies": {
    "consolemd": "^0.2.1",
    "dotenv-safe": "^6.1.0",
    "graphql": "^14.3.0",
    "query-string": "^6.5.0",
    "uWebSockets.js": "github:uNetworking/uWebSockets.js#binaries"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "ava": "^2.0.0",
    "eslint": "^5.16.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
