{
  "name": "react-static-dev-env",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist artifacts tmp",
    "start": "npm run clean && react-static start",
    "stage": "cross-env NODE_ENV=development react-static build --staging",
    "build": "npm run clean && react-static build",
    "analyze": "react-static build --analyze",
    "serve": "serve dist -p 3000",
    "test": "cross-env SKIP_PREFLIGHT_CHECK=true react-scripts test",
    "lint": "eslint ./src",
    "docs": "docz dev",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "classnames": "^2.2.6",
    "core-js": "^3.1.2",
    "intl": "^1.2.5",
    "isomorphic-unfetch": "^3.0.0",
    "lodash.get": "^4.4.2",
    "lodash.mapvalues": "^4.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "^3.0.0",
    "react-redux": "^7.0.3",
    "redaction": "^5.1.1",
    "redux": "^4.0.1",
    "redux-batched-subscribe": "^0.1.6",
    "redux-form": "^8.2.0"
  },
  "devDependencies": {
    "babel-eslint": "9.x",
    "cross-env": "^5.2.0",
    "docz": "^1.0.0-rc.9",
    "docz-plugin-css": "^0.11.0",
    "docz-theme-default": "^1.0.0-rc.9",
    "eslint": "6.x",
    "eslint-config-react-app": "^3.0.8",
    "eslint-config-react-tools": "1.x.x",
    "eslint-plugin-flowtype": "^4.0.0",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "^7.14.1",
    "gh-pages": "^2.0.1",
    "husky": "^3.0.0",
    "lint-staged": "^9.0.0",
    "prettier": "^1.17.1",
    "react-hot-loader": "^4.8.8",
    "react-scripts": "^3.0.1",
    "react-static": "^7.0.10",
    "react-static-plugin-css-modules": "^7.0.10",
    "react-static-plugin-reach-router": "^7.0.10",
    "react-static-plugin-sass": "^7.0.10",
    "react-static-plugin-sitemap": "^7.0.10",
    "react-static-plugin-source-filesystem": "^7.0.10",
    "react-test-renderer": "^16.8.6",
    "serve": "11.1.0",
    "stylelint": "^11.0.0",
    "stylelint-config-sass-guidelines": "^6.0.0"
  }
}
