{
  "name": "graphql-dev-env",
  "version": "0.1.0",
  "description": "An fast & modern GraphQL development environment for with GraphiQL",
  "main": "build/server.js",
  "module": "src/server.js",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development rollup -c -w",
    "start": "node build/server.js",
    "build": "rollup -c",
    "test": "ava",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dalisoft/graphql-dev-env.git"
  },
