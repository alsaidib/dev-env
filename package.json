{
	"name": "preact-unistore-dev-env",
	"version": "0.0.1",
	"license": "MIT",
	"scripts": {
		"start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
		"fix-redirects": "cp ./src/_redirects build/",
		"build": "rm -rf build && preact build --template src/template.html --no-prerender && npm run fix-redirects",
		"dev": "preact watch --template src/template.html",
		"lint": "eslint src",
		"lint:prettier": "prettier -c .prettierrc ./src/*.*",
		"lint:fix": "eslint --fix ./src",
		"lint:prettify": "prettier -c .prettierrc --write ./src/*.*",
		"test": "jest",
		"docs": "docz dev",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"precommit": "npm run lint:prettify && npm run lint:fix"
	},
	"devDependencies": {
		"@namics/stylelint-bem": "^6.0.0",
		"docz": "^1.0.0-rc.9",
		"docz-plugin-css": "^0.11.0",
		"docz-theme-default": "^1.0.0-rc.9",
		"eslint": "^5.16.0",
		"eslint-config-synacor": "^3.0.4",
		"gh-pages": "^2.0.1",
		"html-looks-like": "^1.0.3",
		"husky": "^2.3.0",
		"identity-obj-proxy": "^3.0.0",
		"if-env": "^1.0.4",
		"jest": "^24.8.0",
		"lint-staged": "^8.1.7",
		"node-sass": "^4.12.0",
		"preact-cli": "^2.2.1",
		"preact-render-to-string": "^4.1.0",
		"prettier": "^1.17.1",
		"sass-loader": "^7.1.0",
		"stylelint": "^10.0.1",
		"webpack": "^4.33.0"
	},
	"dependencies": {
		"classnames": "^2.2.6",
		"intl": "^1.2.5",
		"preact": "^8.4.2",
		"preact-compat": "^3.18.5",
		"preact-router": "^2.6.1",
		"react-intl": "^2.9.0",
		"unistore": "^3.4.1"
	}
}
