  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dalisoft/graphql-dev-env/issues"
  },
  "homepage": "https://github.com/dalisoft/graphql-dev-env#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv-safe": "^6.1.0",
    "express-graphql": "^0.8.0",
    "graphql": "^14.4.1",
    "nanoexpress": "^0.9.4"
  },
  "devDependencies": {
    "ava": "^2.0.0",
    "eslint": "^6.0.0",
    "husky": "^3.0.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "rollup": "^1.12.3",
    "rollup-plugin-commonjs": "^10.0.0",
