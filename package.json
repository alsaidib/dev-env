{
  "name": "fastify-dev-env",
  "version": "0.1.0",
  "description": "An modern development environment for Backend development with Fastify",
  "module": "src/server.js",
  "private": true,
  "scripts": {
    "start": "node build/server.js",
    "start:dev": "cross-env NODE_ENV=development nodemon build/server.js",
    "test": "ava",
    "lint": "eslint ./src",
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel dev:server dev:functions dev:serverless dev:now",
    "dev:server": "rollup -c -w",
    "dev:watch-server": "cross-env NODE_ENV=development rollup -c -w",
    "dev:functions": "netlify-lambda serve functions --config webpack.config.netlify.js",
    "dev:serverless": "sls offline",
    "dev:now": "now dev",
    "build": "cross-env NODE_ENV=production npm-run-all --parallel build:**",
    "build:server": "rollup -c",
    "build:functions": "netlify-lambda build functions --config webpack.config.netlify.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dalisoft/fastify-dev-env.git"
  },
  "keywords": [
    "fastify",
    "modern",
    "dev-env"
  ],
  "author": "dalisoft",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dalisoft/fastify-dev-env/issues"
  },
  "homepage": "https://github.com/dalisoft/fastify-dev-env#readme",
  "dependencies": {
    "@dalisoft/cache-ttl": "^0.4.0",
    "@dalisoft/cookie": "0.0.4",
    "apollo-server-fastify": "^2.9.3",
    "aws-serverless-express": "^3.3.6",
    "consolemd": "^0.2.1",
    "dotenv-safe": "^8.1.0",
    "fastify": "^2.8.0",
    "fastify-compress": "^1.0.0",
    "fastify-cookie": "^3.1.0",
    "fastify-cors": "^2.1.3",
    "fastify-helmet": "^3.0.0",
    "fastify-jwt": "^1.1.0",
    "fastify-normalize-request-reply": "^2.1.0",
    "fastify-oas": "^2.3.3",
    "fastify-plugin": "^1.5.0",
    "fastify-rate-limit": "^2.2.0",
    "graphql": "^14.5.4",
    "lodash.pick": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "ava": "^2.3.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.0",
    "eslint": "^6.3.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "netlify-lambda": "^1.6.3",
    "nodemon": "^1.19.2",
    "now": "^16.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.1",
    "rollup": "^1.21.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-run": "^1.1.0",
    "rollup-plugin-terser": "^5.1.1",
    "serverless": "^1.52.0",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.11.0",
    "serverless-webpack": "^5.3.0",
    "serverless-websockets-plugin": "^1.0.0",
    "shebang-loader": "0.0.1",
    "webpack-node-externals": "^1.7.2"
  },
  "optionalDependencies": {
    "@dalisoft/ws-events-sync": "0.0.11",
    "bufferutil": "^4.0.1",
    "long": "^4.0.0",
    "pino-pretty": "^3.0.0",
    "uglify-es": "^3.3.9",
    "utf-8-validate": "^5.0.2",
    "ws": "^7.0.0"
  }
}
