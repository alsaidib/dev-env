{
  "name": "@dalisoft/express-dev-env",
  "version": "0.1.0",
  "description": "An modern and easy development environment for Backend development with Express",
  "module": "src/server.js",
  "private": true,
  "scripts": {
    "start": "node build/server.js",
    "start:dev": "cross-env NODE_ENV=development nodemon build/server.js",
    "test": "ava",
    "lint": "eslint ./src",
    "dev": "cross-env NODE_ENV=development rollup -c -w",
    "build": "rollup -c"
  },
  "keywords": [
    "express",
    "modern",
    "dev-env"
  ],
  "author": "dalisoft",
  "license": "MIT",
  "dependencies": {
    "@dalisoft/ws-events-sync": "0.0.9",
    "consolemd": "^0.2.1",
    "express": "^4.17.1",
    "express-cors": "0.0.3",
    "express-jwt": "^5.3.3",
    "express-rate-limit": "^5.1.3",
    "helmet": "^3.23.2",
    "lodash.pick": "^4.4.0"
  },
  "devDependencies": {
    "ava": "^2.4.0",
    "cross-env": "^6.0.3",
    "dotenv-safe": "^8.2.0",
    "eslint": "^6.8.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rollup": "^1.32.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.4",
    "rollup-plugin-run": "^1.1.0"
  }
}
