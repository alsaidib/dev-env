{
  "name": "@dalisoft/lambda-api-dev-env",
  "version": "0.2.0",
  "description": "An modern development environment for Serverless development with Lambda API",
  "module": "src/server.js",
  "private": true,
  "engine": {
    "node": ">=12.16"
  },
  "scripts": {
    "test": "ava",
    "lint:eslint": "eslint ./src",
    "lint:prettier": "prettier -c .",
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "start": "echo \"This script was deprecated. Please use npm run start:YOUR_CHOICE\" && exit 1",
    "start:http-server": "node --experimental-modules src/http-server.js",
    "dev": "echo \"This script was deprecated. Please use npm run dev:YOUR_CHOICE\" && exit 1",
    "dev:functions": "NODE_ENV=development netlify-lambda serve functions",
    "dev:serverless": "NODE_ENV=development sls offline",
    "dev:vercel": "NODE_ENV=development vercel dev",
    "build": "echo \"This script was deprecated. Please use npm run build:YOUR_CHOICE\" && exit 1",
    "build:functions": "netlify-lambda build functions",
    "build:serverless": "sls webpack"
  },
  "keywords": [
    "lambda",
    "api",
    "serverless",
    "modern",
    "dev-env"
  ],
  "author": "dalisoft",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.706.0",
    "lambda-api": "^0.10.4"
  },
  "devDependencies": {
    "ava": "^3.9.0",
    "eslint": "^7.3.1",
    "husky": "^4.0.2",
    "lint-staged": "^10.2.11",
    "netlify-lambda": "^1.6.3",
    "prettier": "^2.0.5",
    "serverless": "^1.74.0",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^6.4.0",
    "serverless-webpack": "^5.3.1",
    "vercel": "^19.1.1",
    "webpack-node-externals": "^1.7.2"
  }
}
